    "@nestjs/typeorm": "^8.0.2",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "mysql": "^2.18.1",
    "nest-next": "^9.4.0",
    "next": "11",
    "next-redux-wrapper": "^7.0.5",
    "npm-check-updates": "^11.8.5",
    "react": "17",
